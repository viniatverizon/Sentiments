<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
	<class name="com.verizon.hackathon.sentiment.analysis.dataobject.Feedback" table="FEEDBACK">
		<id column="ID" name="id" type="java.lang.Long">
            <generator class="native"/>
        </id>
		<property column="COMMENT" name="comment"/>
		<property column="FEEDBACKDTTM" name="feedbackDTTM" />
		<property column="CREATEDTTM" name="createDTTM" />
	</class>
</hibernate-mapping>